<h1>Passengers</h1>
<h3>All Passengers:</h3>

<%= link_to "Add a new passenger", new_passenger_path %>

<ul>
    <%@passengers.each do |passenger|%>
        <li>
            <p><%= link_to passenger.name, passenger_path(passenger.id) %></p>
            <p>Phone number: <%= passenger.phone_num %></p>
        </li>
    <%end%> 
</ul>